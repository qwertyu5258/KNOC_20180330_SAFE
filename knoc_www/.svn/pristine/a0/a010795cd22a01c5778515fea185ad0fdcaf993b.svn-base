/*	--- list_table ---	*/
.p_common_list_table{
	padding:0;
	border-top:1px solid #7fa1ba;
	border-bottom:2px solid #7fa1ba;
	margin:0 0 10px 0;
	width:675px;
}
.p_common_list_table th{
	border-bottom:2px solid #7fa1ba;
	font-size:11px;
	color:#4b677f;
	padding:3px 0 0 0;
}
.p_common_list_table td{
	padding:5px 3px 3px 3px;
	text-align:center;
	border-bottom:1px dashed #c7c7c7;
}
.p_common_list_table td span{
	font-size:11px;
}
.p_common_list_table td.t_align{
	text-align:left;
}
.p_common_list_table td.t_align a:hover{
	color:#00549a;text-decoration:none;
}

.p_common_list_table td img.event_img{
	border:3px dashed #c7c7c7;
	margin-bottom:2px;
}
/*  ---  행정정보 사진 검색    ---  */
.p_photo_search{
	background:#fbfbfb;
	border-left:1px solid #e4e4e4;
	border-right:1px solid #e4e4e4;
	padding:0 0 0 16px;
}
/*  --- 댓글입력 부분 스타일 시트  ---   */
.p_input_re{
	border:1px solid #c7c7c7;
	background:#FFFFFF;
	font-size:11px;
	font-family:"dotum", "gulim", "Seoul", "arial", "Verdana", "Helvetica";
	padding:3px 3px 1px 3px;
	color:#666666;
}
/*   --- total---  */
