/*	---- view table ---		*/	
.p_common_view_table{
	padding:0;
	border-top:1px solid #7fa1ba;
	border-bottom:1px solid #7fa1ba;
	margin:8px 0 10px 0;
	empty-cells:show;
}
.p_common_view_table th{
	background:#f8fbfd;
	border-bottom:1px solid #dce0e3;
	border-right:1px solid #dce0e3;
	font-size:11px;
	color:#4b677f;
	text-align:left;
	padding:3px 0 0 15px;
}
.p_common_view_table th.center{
	text-align:center;
	padding:10px 0 0 0;
	vertical-align:top;
}
.p_common_view_table th.center01{
	text-align:center;
	padding:0 0 0 0;
	border-right:none;
}
.p_common_view_table th.b_none{
	border-bottom:none;
	border-right:1px solid #dce0e3;
}
.p_common_view_table th.r_none{
	border-right:none;
}
.p_common_view_table td span{
	font-size:11px;
}
.p_common_view_table td{
	border-bottom:1px solid #dce0e3;
	padding:7px 10px 5px 10px;

}
.p_common_view_table td.b_none{
	border-bottom:none;
}
.p_common_view_table td.r_line{
	border-right:1px solid #dce0e3;
}
.p_common_view_table td.center{
	border-right:1px solid #dce0e3;
	text-align:center;
	padding:10px 0 0 0;
	vertical-align:top;
}
.p_common_view_table td.bg_color{
	background:#f8fbfd;
	border-bottom:1px solid #dce0e3;
	padding:7px 10px 5px 10px;
}

.p_common_view_table td p{
	padding:2px;
	text-align:center;
}
.p_common_view_table td p img{
	border:1px solid #b8bbc0;
	padding:2px;
}
.p_common_view_table td.re{
	font-weight:bold;
	color:#096cc1;
	font-size:11px;
	border-top:1px solid #7fa1ba;
	border-bottom:1px solid #7fa1ba;
	background:#f8fbfd;
}
.p_common_view_table td.b{
	padding:5px 0 0 0;
}
.p_board_write_title{
