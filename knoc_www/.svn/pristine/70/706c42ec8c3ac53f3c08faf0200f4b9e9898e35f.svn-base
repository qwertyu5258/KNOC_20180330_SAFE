.main_banner03 ul:after{ display:block; clear:both; content:''; }
.main_banner03 ul li{ float:left; width:90px; height:65px; }
.main_banner03 ul li a{ display:block; overflow:hidden; overflow:hidden; height:30px; padding:35px 0 0; background:url("/img/ico/ico_mainbanner03.png") no-repeat 0 0; line-height:32px; color:#fff; text-align:center; white-space:nowrap; text-overflow:ellipsis; }
.main_banner03 ul li.bg01 a{ background-position:0 0; }
