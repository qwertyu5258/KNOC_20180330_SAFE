@charset "euc-kr"; 
.main #container{ overflow:hidden; position:relative; width:100%; height:885px; background:url(../../img/bg/main_bg.png) no-repeat 50% 0; }
.main #container .main_contents{ position:relative; width:1200px; margin:0 auto; padding-top:10px; }

/* 보도자료/입찰정보 */
.main_board{ position:absolute; top:10px; left:0; width:270px; height:190px; background:url(../../img/bg/main_tab_bg01.png) no-repeat; }
.main_board.tab01{ background-image:url(../../img/bg/main_tab_bg01.png); }
.main_board.tab02{ background-image:url(../../img/bg/main_tab_bg02.png); }
.main_board>ul:after{ display:block; clear:both; content:''; }
.main_board>ul>li{ float:left; }

.main_board>ul>li>button{ display:block; overflow:hidden; width:115px; height:40px; line-height:44px; font-size:1em; color:#6e6e6e; }
.main_board>ul>li.on>button{ font-weight:bold; color:#fff; }
.main_board>ul>li .btn_more{ display:none; position:absolute; top:0; right:0; width:40px; height:40px;  background:url("/img/ico/ico_arrow_knoc.png") no-repeat -200px 0; }
.main_board>ul>li.on .btn_more{display:block;}

.main_board>ul>li>ul{ display:none; position:absolute; top:55px; left:20px; }
.main_board>ul>li.on>ul{ display:block; }
.main_board>ul>li>ul>li{ overflow:hidden; width:225px; height:32px; padding-left:10px; background:url(../../img/ico/arrow.png) no-repeat 0 5px; line-height:16px; font-size:1em; margin-top:10px}
.main_board>ul>li>ul>li:first-child{margin-top:0;}

/* 국내유가/국제유가 */
.main_oil{ position:absolute; top:210px; left:0; width:270px; height:190px; background:url(../../img/bg/main_tab_bg01.png) no-repeat; }
.main_oil.tab01{ background-image:url(../../img/bg/main_tab_bg01.png); }
.main_oil.tab02{ background-image:url(../../img/bg/main_tab_bg02.png); }
.main_oil>ul:after{ display:block; clear:both; content:''; }
.main_oil>ul>li{ float:left; }

.main_oil>ul>li>button{ display:block; overflow:hidden; width:115px; height:40px; line-height:44px; font-size:1em; color:#6e6e6e; }
.main_oil>ul>li.on>button{ font-weight:bold; color:#fff; }
.main_oil>ul>li .btn_more{ display:none; position:absolute; top:0; right:0; width:40px; height:40px; background:url("/img/ico/ico_arrow_knoc.png") no-repeat -200px 0; }
.main_oil>ul>li.on .btn_more{display:block;}
.main_oil>ul>li>div{ display:none; position:absolute; top:40px; left:0; width:230px; padding:10px 20px; }
.main_oil>ul>li.on>div{ display:block; }

.main_oil .depth2{ position:relative; zoom:1; }
.main_oil .depth2:after{ display:block; clear:both; content:''; }
.main_oil .depth2>li{ float:left; margin-right:1px; }
.main_oil .depth2>li>a{ display:block; width:89px; height:20px; background:url(../../img/bg/oil_2depth_bg.png) no-repeat -90px 0; line-height:22px; color:#fff; text-align:center; text-decoration:none; }
.main_oil .depth2>li.on>a{ background-position:0 0; }
.main_oil .depth2>li>div{ display:none; position:absolute; top:35px; left:0; }
.main_oil .depth2>li.on>div{ display:block; }
.main_oil .depth2>li>div>p{ width:230px; height:20px; margin-bottom:13px; background:url(../../img/bg/oil_2depth_data_bg.png) no-repeat; line-height:22px; text-align:center; }
.main_oil .depth2>li>div>ul>li{ position:relative; height:20px; line-height:22px; }
.main_oil .depth2>li>div table th{ height:20px; font-weight:bold; color:#3c3c3c; text-align:left; text-decoration:none; }
.main_oil .depth2>li>div table td{ height:20px; color:#6e6e6e; }
.main_oil .depth2>li>div table td img{ margin-right:10px; vertical-align:middle; }
.main_oil .depth2>li>div table td div {display:inline-block; width:7px; height:4px;margin-right:10px; vertical-align:middle; background:url("/img/ico/ico_arrow_knoc.png") no-repeat; }
.main_oil .depth2>li>div table td div.oil_up {background-position: -130px -7px; }
.main_oil .depth2>li>div table td div.oil_down {background-position: -30px -7px; }

/*.main_oil .depth2>li>div table td div.oil_up img{ margin-right:10px; vertical-align:middle; } */


/* 해외광구도 */
.main_mining{ position:absolute; top:410px; left:0; width:234px; height:54px; padding:18px; background:url(../../img/bg/main_mining_bg.png) no-repeat; }
.main_mining h3{ margin-bottom:6px; font-size:1em; color:#3c3c3c; }
.main_mining select{ width:209px; height:25px; font-size:11px; vertical-align:middle; }
.main_mining a.btn_go_link {display:inline-block; width:25px; height:25px; background:url("/img/btn/btn_sprites_knoc.png") no-repeat -100px -200px; vertical-align:middle;}

/* 메인 비주얼 */
.main_visual{ overflow:hidden; position:relative; width:620px; height:490px; margin:0 auto 10px; }
.main_visual .fade-visual { position:relative; width:620px; height:490px; }
.main_visual .fade-control{ position:absolute; right:10px; bottom:15px; z-index:20; zoom:1; }
.main_visual .fade-control:after{ display:block; clear:both; content:''; }
.main_visual .fade-control button{ display:inline-block; float:left; width:20px; height:20px; background:url(../../img/btn/btn_control.png) no-repeat 0 0; }
.main_visual .fade-control p{ display:inline-block; float:left; height:18px; padding:0 7px; border:1px solid #b4b4b4; border-left:0; background:#fff; line-height:20px; font-size:11px; color:#6e6e6e; }
.main_visual .fade-control p .cur_num{ font-weight:bold; color:#e30009; }
.main_visual .fade-control button.fade-prev{ background-position:0 0; }
.main_visual .fade-control button.fade-resume{ background-position:-20px 0; }
.main_visual .fade-control button.fade-pause{ background-position:-40px 0; }
.main_visual .fade-control button.fade-next{ background-position:-60px 0; }
.main_visual .fade-control button.fade-hidden{display:none}
