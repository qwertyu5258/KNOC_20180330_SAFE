@charset "euc-kr" ;

/* common */
* {margin:0 ; padding:0 ; font-family:"µ¸¿ò", dotum, sans-serif ;}
body {font-size:12px ; color:#666 ;}
input, textarea, select, option {font-size:12px ; color:#9a9a9a ;}
table, td, th {font-size:12px ;}
img, fieldset {border:none ;}
li {list-style:none ;}
a {cursor:pointer ;}
a:link, a:visited {text-decoration:none ; color:#666 ;}
a:hover, a:active, a:focus {text-decoration:underline ; color:#000 ;}
hr {display:none ;}
legend {font-size:0 ; width:0 ; height:0 ; display:block ; position:absolute ; left:0 ; top:-5000px ; overflow:hidden ;}
h1, h2, h3, h4, h5, h6 {font-size:12px ; color:#666 ;}

/* class */
.blind {display:block ; position:absolute ; left:0 ; top:-5000px ; overflow:hidden ;}
.test {border:1px solid red ;}
